rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    
    // Development rules - more permissive for testing
    match /questions/{questionId} {
      allow read, write: if request.auth != null;
    }
    
    match /customTags/{userId} {
      allow read, write: if request.auth != null;
    }
  }
}
